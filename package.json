{
  "name": "html-and-css-practice",
  "version": "1.0.0",
  "homepage": "",
  "repository": "",
  "author": "Roman Dmiterko",
  "license": "MIT",
  "scripts": {
    "dev": "parcel *.html",
    "build": "parcel build *.html --public-url './'",
    "deploy": "yarn build && gh-pages -d dist",
    "lint": "eslint ./js/*",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,less}": "prettier --write"
  },
  "devDependencies": {
    "@eslint/js": "9.16.0",
    "@parcel/transformer-less": "2.13.2",
    "eslint": "9.16.0",
    "gh-pages": "6.2.0",
    "globals": "15.13.0",
    "husky": "9.1.7",
    "less": "4.2.1",
    "lint-staged": "15.2.10",
    "parcel": "2.13.2",
    "prettier": "3.4.2"
  },
  "packageManager": "yarn@4.5.3"
}
